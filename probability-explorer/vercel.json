{
  "version": 2,
  "name": "yhkc-apps",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/probability-explorer",
      "dest": "/probability-explorer/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}